{
  "name": "compose-market-lambda",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "esbuild handler.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/index.js --minify",
    "deploy": "npm run build && npx serverless deploy --verbose",
    "deploy:domain": "npx serverless create_domain",
    "deploy:quick": "tsx deploy.ts",
    "dev": "tsx index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@ai-sdk/google": "^2.0.45",
    "@ai-sdk/openai": "^2.0.80",
    "@ai-sdk/openai-compatible": "^1.0.28",
    "@huggingface/hub": "^2.7.1",
    "@huggingface/inference": "^4.13.5",
    "ai": "^5.0.108",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "mem0ai": "^2.1.38",
    "thirdweb": "^5.115.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.2",
    "esbuild": "^0.27.1",
    "serverless": "^4.27.1",
    "serverless-domain-manager": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
